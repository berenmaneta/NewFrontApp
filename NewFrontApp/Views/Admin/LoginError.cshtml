                    <button type="reset" class="btn btn-default btn-sm b-0" onclick="location.href='@Url.Action("Login", "Admin")'">Try again</button>
                    <button type="reset" class="btn btn-greensea btn-sm b-0" onclick="location.href='@Url.Action("Index", "Home")'">Return to Home</button>
                </div>
            </div>
        </div>
    </div>
